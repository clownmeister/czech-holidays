{
  "name": "@clownmeister/czech-holidays",
  "type": "module",
  "version": "1.0.0-b7",
  "description": "Provides Czech holidays with no historical data. Supports local storage caching.",
  "repository": {
    "url": "https://github.com/clownmeister/czech-holidays"
  },
  "author": "ClownMeister",
  "publishConfig": {
    "access": "public",
    "@clownmeister:registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/czech-holidays.esm.js",
  "types": "dist/czech-holidays.d.ts",
  "exports": {
    ".": {
      "import": "./dist/czech-holidays.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --check ./src",
    "format:fix": "prettier --write ./src"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.9.0",
    "vitest": "^1.5.3"
  }
}
